<view class="login"><view class="top"><view class="logo"><image src="../../static/image/logo.png"></image></view></view><view class="content"><view class="input-text"><text class="ico iconfont iconyonghuming"></text><input class="uni-input" focus="{{true}}" placeholder="账号" data-event-opts="{{[['input',[['__set_model',['$0','userName','$event',[]],['userLoginInfo']]]]]}}" value="{{userLoginInfo.userName}}" bindinput="__e"/></view><view class="input-text"><text class="ico iconfont iconmima"></text><input class="uni-input" password="{{true}}" type="text" placeholder="密码" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['userLoginInfo']]]]]}}" value="{{userLoginInfo.password}}" bindinput="__e"/></view><view class="sign"><button type="primary" disabled="{{!userLoginInfo.userName||!userLoginInfo.password}}" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">登录</button></view></view></view>